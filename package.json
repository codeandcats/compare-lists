{
  "name": "compare-lists",
  "version": "1.0.3",
  "description": "Super efficiently compares two sorted lists (arrays, strings, anything that is iterable actually).",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.json",
    "clean": "rimraf ./dist",
    "check": "npm run lint && npm run build && npm test",
    "commit": "commit",
    "lint": "tslint -p ./tsconfig.json",
    "test": "jest --coverage",
    "prepackage": "npm run build",
    "package": "ts-node ./support/package.ts",
    "release": "git checkout master && git pull origin master && standard-version",
    "preship-it": "npm run package",
    "ship-it": "git push --follow-tags origin master && cd ./dist && npm publish"
  },
  "keywords": [
    "compare",
    "collections",
    "lists",
    "arrays",
    "maps",
    "strings",
    "iterable",
    "iterator"
  ],
  "author": "Ben <codeandcats@gmail.com>",
  "license": "MIT",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/prompt-cli": "^7.2.1",
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.7",
    "app-root-path": "^2.1.0",
    "husky": "^1.1.2",
    "jest": "^23.4.2",
    "replace": "^1.0.0",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandcats/compare-lists.git"
  },
  "bugs": {
    "url": "https://github.com/codeandcats/compare-lists/issues"
  },
  "homepage": "https://github.com/codeandcats/compare-lists#readme"
}
